{
    "interval": 2,
    
    "measure": "kb",
    
    "cpu_fields": ["system", "user", "nice", "idle", "iowait", "irq", "softirq", "steal", "guest", "guest_nice", "freq"],
    "cpu_extended": true,
    
    "net_fields": ["bytes", "packets", "errors", "drops"],
    "net_extended": true,
    
    "mem_fields": ["used", "buffers", "cached", "shared"],
    "mem_extended": true,
    
    "dsk_fields": ["used", "read", "write"],
    "dsk_extended": true
}
